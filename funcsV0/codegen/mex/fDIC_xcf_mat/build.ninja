cxxflags =  -c -fexceptions -fno-omit-frame-pointer -m64 -DMATLAB_MEX_FILE  -DMATLAB_MEX_FILE -O2 -fwrapv -DNDEBUG -std=c++11 -I "D:\RockShare\Work\Projects\2025_Contiguity_SECO\Technical\standalone-app-2.0\mtex-trueEbsd-2.0.0\funcsV0\codegen\codegen\mex\fDIC_xcf_mat" -I "D:\RockShare\Work\Projects\2025_Contiguity_SECO\Technical\standalone-app-2.0\mtex-trueEbsd-2.0.0\funcsV0\codegen" -I "D:\RockShare\Work\Projects\2025_Contiguity_SECO\Technical\standalone-app-2.0\mtex-trueEbsd-2.0.0\funcsV0\codegen\codegen\mex\fDIC_xcf_mat\interface" -I "C:\Program Files\MATLAB\R2025a\extern\include" -I "." -DMODEL=fDIC_xcf_mat_mex -D__USE_MINGW_ANSI_STDIO=1 -msse2 -fno-predictive-commoning -Wno-stringop-overflow -Wno-maybe-uninitialized -fopenmp 

ldflags = fDIC_xcf_mat.def  -m64 -Wl,--no-undefined -shared -static -L"C:\Program Files\MATLAB\R2025a\extern\lib\win64\mingw64" -llibmx -llibmex -llibmat -lm -llibmwlapack -llibmwblas -o "fDIC_xcf_mat_mex.mexw64" -L"$matlabdir\extern\lib\win64\mingw64" -llibmwblas -llibemlrt -llibcovrt -llibut -llibmwmathutil -fopenmp

