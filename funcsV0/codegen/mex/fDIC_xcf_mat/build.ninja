# Link
build $root\fDIC_xcf_mat_mex.mexw64 : link $builddir/fDIC_xcf_mat_data.o $builddir/rt_nonfinite.o $builddir/fDIC_xcf_mat_initialize.o $builddir/fDIC_xcf_mat_terminate.o $builddir/fDIC_xcf_mat.o $builddir/blockedSummation.o $builddir/sumMatrixIncludeNaN.o $builddir/std.o $builddir/fft2.o $builddir/fReg.o $builddir/ifft2.o $builddir/relop.o $builddir/indexShapeCheck.o $builddir/mtimes.o $builddir/assertValidSizeArg.o $builddir/_coder_fDIC_xcf_mat_api.o $builddir/_coder_fDIC_xcf_mat_mex.o $builddir/eml_int_forloop_overflow_check.o $builddir/eml_fftshift.o $builddir/ifftshift.o $builddir/exp.o $builddir/fDIC_xcf_mat_emxutil.o $builddir/_coder_fDIC_xcf_mat_info.o $builddir/c_mexapi_version.o 
